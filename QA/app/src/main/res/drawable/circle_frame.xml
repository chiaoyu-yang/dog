<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--第一层阴影-->
    <item>
        <shape android:shape="oval">
            <solid android:color="#0F000000"/>
        </shape>
    </item>
    <!--第二层前景-->
    <item
        android:bottom="5dp"
        android:left="3dp"
        android:right="5dp"
        android:top="3dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>