<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.REQUEST_NOTIFICATION_POLICY" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.READ_PHONE_STATE"
        tools:node="remove" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/Theme.Soulgo"
        tools:targetApi="31">
        <activity
            android:name=".Quiz.QuizResultDireation"
            android:exported="false" />
        <activity
            android:name=".Quiz.QuizResults"
            android:exported="false" />
        <activity
            android:name=".Quiz.QuizActivity"
            android:exported="false" />
        <activity
            android:name=".HomeActivity"
            android:exported="false" />
        <activity
            android:name=".Rank.RankingActivity"
            android:exported="false" />
        <activity
            android:name=".Rank.RankDirectionsActivity"
            android:exported="false" />
        <activity
            android:name=".News.NewsActivity"
            android:exported="false" />
        <activity
            android:name=".News.PostActivity"
            android:exported="false" />
        <activity
            android:name=".News.PublishActivity"
            android:exported="false" />
        <activity
            android:name=".Book.BookActivity"
            android:exported="false" />
        <activity
            android:name=".Book.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".Book.BookDireationsActivity"
            android:exported="false" />
        <activity
            android:name=".Book.DetailDireationsActivity"
            android:exported="false" />
        <activity
            android:name=".Beauty.VoteActivity"
            android:exported="false" />
        <activity
            android:name=".Beauty.BeautyActivity"
            android:exported="false" />
        <activity
            android:name=".Beauty.VotePublish"
            android:exported="false" />
        <activity
            android:name=".Beauty.BeautyDireationsActivity"
            android:exported="false" />
        <activity
            android:name=".Beauty.VotePublishDireation"
            android:exported="false" />
        <activity
            android:name=".Setting.SettingActivity"
            android:exported="false" />
        <activity
            android:name=".Setting.SettingDireationsActivity"
            android:exported="false" />
        <activity
            android:name=".Identify.ClassifierActivity"
            android:exported="false"
            android:label="@string/activity_name_classification"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/*" />
            </intent-filter>
        </activity>
        <activity android:name=".Identify.SimpleListActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <service android:name=".Setting.BackgroundMusicService" />
        <service android:name=".Setting.Beep" />
        <service
            android:name=".Setting.ReminderJobIntentService"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <receiver android:name=".Setting.ReminderBroadcast" />
    </application>

</manifest>
